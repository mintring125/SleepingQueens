{
  "rewrites": [
    { "source": "/manifest-home.json", "destination": "/client/manifest-home.json" },
    { "source": "/manifest.json", "destination": "/client/manifest.json" },
    { "source": "/manifest-table.json", "destination": "/client/manifest-table.json" },
    { "source": "/sw.js", "destination": "/client/sw.js" },
    { "source": "/icons/(.*)", "destination": "/client/icons/$1" },
    { "source": "/assets/(.*)", "destination": "/client/assets/$1" },
    { "source": "/shared/(.*)", "destination": "/client/shared/$1" },
    { "source": "/table/(.*)", "destination": "/client/table/$1" },
    { "source": "/player/(.*)", "destination": "/client/player/$1" },
    { "source": "/co-bingo", "destination": "/client/co-bingo/index.html" },
    { "source": "/co-bingo/", "destination": "/client/co-bingo/index.html" },
    { "source": "/co-bingo/(.*)", "destination": "/client/co-bingo/$1" },
    { "source": "/dora-othello", "destination": "/client/dora-othello/index.html" },
    { "source": "/dora-othello/", "destination": "/client/dora-othello/index.html" },
    { "source": "/dora-othello/(.*)", "destination": "/client/dora-othello/$1" },
    { "source": "/도라에몽오델로", "destination": "/client/dora-othello/index.html" },
    { "source": "/도라에몽오델로/", "destination": "/client/dora-othello/index.html" },
    { "source": "/도라에몽오델로/(.*)", "destination": "/client/dora-othello/$1" },
    { "source": "/kariba/table/(.*)", "destination": "/client/kariba/table/$1" },
    { "source": "/kariba/player/(.*)", "destination": "/client/kariba/player/$1" },
    { "source": "/kariba/shared/(.*)", "destination": "/client/kariba/shared/$1" },
    { "source": "/kariba/assets/(.*)", "destination": "/client/kariba/assets/$1" },
    { "source": "/kariba/table", "destination": "/client/kariba/table/index.html" },
    { "source": "/kariba/player", "destination": "/client/kariba/player/index.html" },
    { "source": "/kariba", "destination": "/client/kariba/table/index.html" },
    { "source": "/HaliGali", "destination": "/client/HaliGali/index.html" },
    { "source": "/HaliGali/(.*)", "destination": "/client/HaliGali/$1" },
    { "source": "/haligali", "destination": "/client/HaliGali/index.html" },
    { "source": "/haligali/(.*)", "destination": "/client/HaliGali/$1" },
    { "source": "/", "destination": "/client/index.html" },
    { "source": "/table", "destination": "/client/table/index.html" },
    { "source": "/player", "destination": "/client/player/index.html" },
    { "source": "/player/join", "destination": "/client/player/join.html" },
    { "source": "/join", "destination": "/client/player/join.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    },
    {
      "source": "/icons/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400" }
      ]
    }
  ]
}
